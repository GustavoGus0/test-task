{
  "name": "@management/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.app.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --fix",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache src/**/*.scss",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --log-level=warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' !**/{node_modules,dist}/**/*",
    "checkall": "pnpm prettify && pnpm lint && pnpm types && pnpm stylelint && pnpm stylecheck"
  },
  "dependencies": {
    "@management/backend": "workspace:*",
    "@tanstack/react-query": "^5.74.4",
    "@trpc/client": "^11.1.0",
    "@trpc/react-query": "^11.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.9.2",
    "js-cookie": "^3.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.5.1",
    "superjson": "^2.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "classnames": "^2.5.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "sass": "^1.86.3",
    "stylelint": "^16.18.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-scss": "^6.11.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1",
    "usehooks-ts": "^3.1.1",
    "vite": "^6.3.1",
    "zod": "^3.24.3"
  }
}
